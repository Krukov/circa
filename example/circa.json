{
  "storages": {
    "r": "redis://redis.default.svc.cluster.local/0"
  },
  "rules": {
    "/": {
      "random:key": {
        "ttl": "1m"
      }
    },
    "/me": {
      "me:key": {
        "ttl": "1m"
      }
    },
    "/*": {
      "{request_path}:fail": {
        "ttl": "1m",
        "type": "fail"
      }
    }
  },
  "options": {
    "host": "http://localhost:8000",
    "timeout": "20s"
  }
}